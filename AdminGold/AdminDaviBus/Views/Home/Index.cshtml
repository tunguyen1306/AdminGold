<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
               
            
                <div class="form-group row" style="margin-bottom:0!important;">
                    <div class="col-lg-4 divCity">
                        <label>Tỉnh/Thành:</label>
                        <select class="btn btn-default btn-xs dropdown-toggle dropCity" style="height:35px; background-color:#fff">
                            <option>Chọn tỉnh/thành</option>
                        </select>
                    </div>
                    <div class="col-lg-4 divDistrict hidden">
                        <label>Quận/huyện:</label>
                        <select class="btn btn-default btn-xs dropdown-toggle dropDistrict" style="height:35px;background-color:#fff">
                            <option>Chọn quận/huyện</option>
                        </select>
                    </div>
                    <div class="col-lg-4 divWard hidden">
                        <label>Phường/Xã:</label>
                        <select class="btn btn-default btn-xs dropdown-toggle dropWard" style="height:35px;background-color:#fff">
                            <option>Chọn phường/xã</option>
                        </select>
                    </div>
                </div>
               
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div id="map" style="height:800px"></div>
                  
              
            </div>
            <script async defer
                    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCbIWQ_WqsJHtBKUy687lw8l1NIgQrBa7M&callback=initMap">
            </script>       
          
            
           
            <!-- /.panel-body -->
        </div>

    </div>

</div>
