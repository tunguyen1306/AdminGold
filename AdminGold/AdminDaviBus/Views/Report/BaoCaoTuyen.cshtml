@model IEnumerable<AdminGold.Models.DMTUYEN>

@{
    ViewBag.Title = "BaoCaoTuyen";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Báo cáo tuyến xe</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
              
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>Mã tuyến</th>
                          

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {


                                <tr class="odd gradeX">
                                    <td>
                                        <a href="@Url.Action("LoTrinh","Home",new {id= item.IDTUYEN})">@Html.DisplayFor(modelItem => item.MATUYEN)</a>
                                    </td>
                                    <td>
                                       @Html.DisplayFor(modelItem => item.TENTUYENVN)
                                    </td>

                                </tr>
                            }
                           
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>



